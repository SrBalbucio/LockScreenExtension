{
    "manifest_version": 3,
    "name": "LockScreen",
    "description": "Extensão para bloquear o seu navegador quando necessário",
    "version": "1.0",
    "action":{
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions":[
        "activeTab", "cookies", "storage", "management", "tabs", "alarms"
    ],
    "background": {
        "service_worker": "scripts/worker.js"
    }
}